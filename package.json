{
  "name": "devflow-ai",
  "version": "1.0.0",
  "description": "AI-Powered Developer Assistant - 가장 핫한 개발자 생산성 도구",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "npm run server:start",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm run start",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "ai",
    "developer-tools",
    "code-review",
    "automation",
    "github",
    "typescript",
    "react",
    "nodejs"
  ],
  "author": {
    "name": "박준철",
    "email": "jun22sky@junetapa.com",
    "url": "https://junetapa.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/junetapa-juncheol/DevFlow-AI.git"
  },
  "bugs": {
    "url": "https://github.com/junetapa-juncheol/DevFlow-AI/issues"
  },
  "homepage": "https://github.com/junetapa-juncheol/DevFlow-AI#readme",
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}